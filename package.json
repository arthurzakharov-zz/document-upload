{
  "name": "document-upload",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://arthurzakharov.github.io/document-upload/",
  "scripts": {
    "start": "npm run svgr && react-scripts start",
    "build": "npm run svgr && react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:snapshot": "react-scripts test --watchAll=false --updateSnapshot",
    "test:coverage": "react-scripts test --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "svgr": "svgr --typescript --no-dimensions --no-index -d src/svg src/assets/svg",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**",
      "!src/**/*.test.tsx.snap",
      "!src/**/index.ts",
      "!src/svg/**",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/react-app-env.d.ts"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fontsource/open-sans": "^4.5.10",
    "@fontsource/rubik": "^4.5.9",
    "@fontsource/titillium-web": "^4.5.8",
    "@reduxjs/toolkit": "^1.8.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "classnames": "^2.3.1",
    "react": "^18.1.0",
    "react-animate-height": "^2.1.2",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.1",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.2",
    "redux-logger": "^3.0.6",
    "typescript": "^4.6.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@svgr/cli": "^6.2.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^16.11.34",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-redux": "^7.1.24",
    "@types/react-transition-group": "^4.4.4",
    "@types/redux-logger": "^3.0.9",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.0",
    "prettier": "^2.6.2",
    "redux-mock-store": "^1.5.4"
  }
}
